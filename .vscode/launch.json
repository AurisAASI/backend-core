    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Lambda: city collector",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "-f", "city_collector",
                "-e", "${workspaceFolder}/.vscode/event-city_collector.json"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
